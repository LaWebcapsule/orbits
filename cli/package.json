{
    "name": "@orbi-ts/cli",
    "version": "0.0.2",
    "description": "Cli to run your Orbits App",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LaWebcapsule/orbits"
    },
    "author": "la webcapsule",
    "license": "ISC",
    "scripts": {
        "test": "echo \"Info: no test specified\"",
        "format": "prettier --check --ignore-unknown **/*.ts",
