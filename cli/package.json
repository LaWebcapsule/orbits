    "devDependencies": {
        "@tsconfig/node22": "^22.0.2",
        "@types/blessed": "^0.1.25",
        "@types/copy-paste": "^2.1.0",
        "prettier": "^3.6.2",
        "prettier-plugin-organize-imports": "^4.2.0",
        "tslib": "catalog:",
        "tsx": "^4.20.3",
        "typescript": "^5.9.2"
    },
    "files": [
        "dist"
    ],
    "bin": {
        "orbits-cli": "dist/bin/index.js"
